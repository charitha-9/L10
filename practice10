#include<stdio.h>
struct student{
  char name[50];
  int age;
  char section;
  float marks;
  };
  int main(){
  struct student s;

  printf("enter name :");
  scanf("%s",s.name);
  printf("enter age:");
  scanf("%d",s.age);
  printf("enter section :");
  scanf("%s",s.section);
  printf("enter marks :");
  scanf("%d",s.marks);


   printf("\nStudent Info:\n");
   printf("name: %s\n Age: %d\n Marks: %.2f\n",s.name,s.age,s.marks);
   return 0;
   }  
