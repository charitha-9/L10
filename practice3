#include<stdio.h>
int main()
{
  int a;
  int b;
  printf("enter the number:");
  scanf("%d",&a);
  printf("enter the number:");
  scanf("%d",&b);
  printf("before swapping value of a and b is %d and %d",a,b);
  a=a+b;
  b=a-b;
  a=a-b;
  printf("after swapping value of a and b is %d and %d",a,b);
  return 0;
 }
